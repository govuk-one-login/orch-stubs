{
  "name": "orch-stubs",
  "version": "1.0.0",
  "description": "Performance and scaling stubs owned by Orchestration",
  "author": "",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "sam build",
    "start:local": "sam local start-api",
    "start:local:warm": "sam local start-api --warm-containers EAGER",
    "check": "npm run check:lint && npm run check:prettier",
    "check:lint": "eslint .",
    "check:prettier": "prettier --check \"**/*.{ts,json}\"",
    "fix": "npm run fix:lint && npm run fix:prettier",
    "fix:lint": "eslint . --fix",
    "fix:prettier": "prettier --write \"**/*.{ts,json}\"",
    "test": "jest --passWithNoTests",
    "test:coverage": "jest --coverage --passWithNoTests",
    "prepare": "husky"
  },
  "prettier": {
    "trailingComma": "es5",
    "tabWidth": 2,
    "semi": true
  },
  "dependencies": {
    "esbuild": "^0.23.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.7.0",
    "@types/aws-lambda": "^8.10.141",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.14.10",
    "@types/supertest": "^6.0.2",
    "eslint": "^8.57.0",
    "eslint-plugin-jest": "^28.6.0",
    "globals": "^15.8.0",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "prettier": "^3.3.2",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.0",
    "typescript": "^5.5.3",
    "typescript-eslint": "^7.16.1"
  }
}
